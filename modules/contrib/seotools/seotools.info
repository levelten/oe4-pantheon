name = "SEO Tools"
description = "Suite of advanced search engine optimization tools for Drupal"
core = "7.x"
package = "SEO"
php = "5.2.4"
dependencies[] = "alchemy"
dependencies[] = "alchemy_contentanalysis"
dependencies[] = "contentanalysis"
dependencies[] = "contentoptimizer"
dependencies[] = "features"
dependencies[] = "globalredirect"
dependencies[] = "insight"
dependencies[] = "kwresearch"
dependencies[] = "kwresearch_google"
dependencies[] = "libraries"
dependencies[] = "metatag"
dependencies[] = "pathauto"
dependencies[] = "redirect"
dependencies[] = "strongarm"
dependencies[] = "xmlsitemap"
dependencies[] = "xmlsitemap_node"
configure = "admin/config/search/seotools"
features[ctools][] = "strongarm:strongarm:1"
features[variable][] = "insight_contentanalysis_autorun"
features[variable][] = "insight_contentanalysis_autorun_analyzers"
apps[server] = 'development'
apps[manifests][] = 'app/manifest.app'