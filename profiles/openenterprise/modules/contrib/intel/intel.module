function intel_add_visitor_sync_request($vtk, $delay = 0) {
  $items = variable_get('intel_sync_visitor_requests', array());
  if (!isset($items[$vtk])) {
    $item = new stdClass;
    $item->created = time();
    $item->run_after = time() + $delay;
    $item->vtk = $vtk;
    $item->attempts = 0;
    $items[$vtk] = $item;
    variable_set('intel_sync_visitor_requests', $items);
  }
}

