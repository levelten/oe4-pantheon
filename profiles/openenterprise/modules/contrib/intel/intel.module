function intel_path_filter_type_to_arg($arg) {
  if (arg(0) == 'node') {
    return 'page';
  }
  $a = arg(5);
  return isset($a) ? $a : '';
}

function intel_path_filter_value_to_arg($arg) {
  if (arg(0) == 'node') {
    $path = url('node/' . arg(1));
    return "landingPagePath:" . urlencode($path);
  }
  $a = arg(6);
  return isset($a) ? $a : '';
}

